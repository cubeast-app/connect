{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "cubeast-connect",
  "version": "0.4.0",
  "identifier": "com.cubeast.connect",
  "build": {
    "beforeDevCommand": "yarn start",
    "devUrl": "http://localhost:4200",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist/cubeast-connect/browser"
  },
  "app": {
    "windows": [
      {
        "title": "Cubeast Connect",
        "width": 800,
        "height": 600,
        "resizable": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "createUpdaterArtifacts": true
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDdDNkI2OEU3QzZEMjkxQ0QKUldUTmtkTEc1MmhyZk53bnViR1BPR3JoV0JBcjRMSjF3QXRjUFVpNWtiOGU0U0tmWW5Ha09IZlIK",
      "endpoints": [
        "https://gist.githubusercontent.com/tomekpiotrowski/6e229a4e9a589a9f4410a6ef86fa9f6b/raw/cubeast-connect-updater.json"
      ]
    }
  }

}
