{
  "build": {
    "beforeDevCommand": "yarn start --port 1420",
    "beforeBuildCommand": "yarn build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist/cubeast-connect",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "Cubeast Connect",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/icon.png",
        "icons/icon.ico"
      ],
      "identifier": "com.cubeast.connect",
      "targets": "all"
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "systemTray": {
      "iconPath": "icons/icon.png",
      "iconAsTemplate": true
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": false,
        "title": "Cubeast Connect",
        "width": 600,
        "height": 400
      }
    ]
  }
}
